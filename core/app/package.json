  "scripts": {
    "build-css": "less-watch-compiler --source-map --run-once src src",
    "watch-css": "npm run build-css && less-watch-compiler --source-map src src",
    "start-js": "react-scripts-ts start",
    "start-demo": "cd ../../demo && cross-env ASPNETCORE_ENVIRONMENT=Development ASPNETCORE_URLS=\"http://*:5000\" dotnet run",
    "start-demo-js": "cross-env REACT_APP_API_URL=http://localhost:5000/orders react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "start-all": "npm-run-all -p watch-css start-demo-js start-demo",
    "localization-clean": "rimraf extracted",
    "localization-compile": "tsc -p .  --target ES6 --module esNext --jsx preserve --outDir extracted",
    "localization-extract": "babel --plugins dynamic-import-webpack \"extracted/**/*.jsx\"",
    "localization-manage": "node scripts/translationRunner.js",
    "localization": "npm-run-all localization-clean localization-compile localization-extract localization-manage localization-clean",
    "build-js": "react-scripts-ts build",
    "build-api": "cd ../api && ASPNETCORE_ENVIRONMENT=Development dotnet build",
    "build-demo": "cd ../demo && ASPNETCORE_ENVIRONMENT=Development dotnet build",
    "build": "npm-run-all localization build-css build-js",
    "test-ts": "react-scripts-ts test --env=jsdom",
    "test-api": "cd ../api.test && ASPNETCORE_ENVIRONMENT=Development  dotnet test",
    "test": "npm-run-all -s test-ts test-api",
    "eject": "react-scripts-ts to-take-out-if-you-want-to-eject eject"
  },
